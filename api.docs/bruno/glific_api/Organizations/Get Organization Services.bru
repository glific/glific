meta {
  name: Get Organization Services
  type: graphql
  seq: 3
}

post {
  url: {{api_url}}
  body: graphql
  auth: none
}

headers {
  authorization: {{auth_token}}
  Content-Type: application/json
}

body:graphql {
  
  query organizationServices() {
    bigquery
    dialogflow
    googleCloudStorage
    funWithFlags
    flowUuidDisplay
    errors {
      key
      message
    }
  }
}
